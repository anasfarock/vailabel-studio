{
  "name": "vailabel",
  "version": "1.0.0",
  "description": "image labeling",
  "private": true,
  "authors": {
    "name": "Vichea Nath",
    "email": "nathvichea1@gmail.com"
  },
  "scripts": {
    "clean": "npx git-clear-branch",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --ignore-path .gitignore --ignore-path .prettierignore",
    "commit": "cz",
    "prepare": "husky install",
    "test": "jest --coverage",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:web": "yarn workspaces web dev",
    "build:web": "yarn workspaces web build",
    "release": "changeset publish"
  },
  "keywords": [],
  "workspaces": [
    "apps/*"
  ],
  "nohoist": [
    "**"
  ],
  "author": {
    "name": "Vichea Nath",
    "email": "nathvichea1@gmail.com"
  },
  "license": "GPL-3.0-only",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@types/jest": "^29.5.14",
    "changesets": "^1.0.2",
    "commitizen": "^4.3.1",
    "concurrently": "^9.2.0",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "turbo": "^2.5.5"
  },
  "packageManager": "pnpm@8.9.2"
}
