{
  "name": "vailabel",
  "version": "1.0.0",
  "description": "image labeling",
  "private": true,
  "authors": {
    "name": "Vichea Nath",
    "email": "nathvichea1@gmail.com"
  },
  "scripts": {
    "clean": "npx git-clear-branch",
    "format": "turbo run format",
    "commit": "cz",
    "prepare": "husky install",
    "test": "turbo run test",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "studio": "turbo run dev --filter=@vailabel/studio  --filter=@vailabel/api",
    "desktop": "turbo run dev --filter=vailabel-desktop --filter=@vailabel/studio",
    "release": "turbo run release --filter=@vailabel/studio --filter=vailabel-desktop",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint"
  },
  "keywords": [],
  "workspaces": [
    "apps/*"
  ],
  "author": {
    "name": "Vichea Nath",
    "email": "nathvichea1@gmail.com"
  },
  "license": "GPL-3.0-only",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@types/jest": "^30.0.0",
    "@types/semver": "^7.7.0",
    "changesets": "^1.0.2",
    "commitizen": "^4.3.1",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "turbo": "^2.5.6"
  },
  "packageManager": "yarn@4.0.0"
}
