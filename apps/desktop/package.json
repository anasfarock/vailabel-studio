{
  "name": "desktop",
  "productName": "desktop",
  "version": "0.1.3",
  "description": "Vailabel Studio Desktop",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "copyfiles -u 3 \"../studio/dist/**/*\" ./dist",
    "start": "tsc && electron .",
    "build": "tsc && electron-builder",
    "build:publish": "yarn prebuild && tsc && electron-builder --publish always",
    "watch": "tsc --watch & electronmon . || nodemon --exec electron . --ext js,ts,json,html"
  },
  "keywords": [],
  "author": {
    "name": "Vichea",
    "email": "nathvichea1@gmail.com",
    "url": "https://github.com/vicheanath"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/semver": "^7.7.0",
    "electron": "36.2.1",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "electron-updater": "^6.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vailabel/vailabel-studio.git"
  }
}
