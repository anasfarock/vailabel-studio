{
  "name": "desktop",
  "productName": "desktop",
  "version": "0.1.6",
  "description": "Vailabel Studio Desktop",
  "main": "dist/main.js",
  "scripts": {
    "copy-python": "copyfiles -u -1 ./src/ai/requirements.txt ./dist/ai && copyfiles -u -1 ./src/ai/yolo.py ./dist/ai",
    "copy-migration": "copyfiles -u 3 src/db/migrations/**/* dist/db/migrations",
    "pre-dev": "yarn copy-python && yarn copy-migration",
    "build": "tsc && electron-builder",
    "dev": "yarn pre-dev && tsc && electron . --trace-warnings",
    "test": "jest",
    "prebuild": "npx electron-icon-maker --input=./assets/icon.png --output=./assets/icon"
  },
  "keywords": [],
  "author": {
    "name": "Vichea",
    "email": "nathvichea1@gmail.com",
    "url": "https://github.com/vicheanath"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/semver": "^7.7.0",
    "@vailabel/core": "*",
    "electron": "36.2.1",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "electron-updater": "^6.6.2",
    "keytar": "^7.9.0",
    "sqlite3": "^5.1.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vailabel/vailabel-studio.git"
  }
}
